<!doctype html>
<html>
    <head>
        <title>Modbus Server Data</title>
        <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css">
    </head>
    <body>
        <div class="container"> 

                <div class="jumbotron">
                        <h1>Server Data On <%=serverData[0].date%></h1>
                </div>
                
        
                <table class="table table-striped table-bordered">
                    <tr>
                       <th>S.No</th>
                       <th>Parameter</th>
                       <th>Address</th>
                       <th>Value</th>
                       <th>Hexadecimal</th>
                       
                    </tr>
                 
                    <% for (var i = 0; i < serverData.length; i++) { %>
                     <tr>
                       <td><%= i+1 %></td>
                       <td><a href="/serverData/<%=serverData[i].timeOfCreation%>/<%=serverData[i].address%>"><%= serverData[i].parameter %></a></td>
                       <td><%= serverData[i].address %></td>
                       <td><%=serverData[i].value%></td>
                       <td><%=serverData[i].hexValue%></td>
                       
                       
                     </tr>    
                    <% } %>
                 </table>
        </div>
        
    </body>
</html>
